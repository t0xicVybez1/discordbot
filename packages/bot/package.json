{
  "name": "@discordbot/bot",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "main": "./dist/index.js",
  "scripts": {
    "dev": "tsx watch --env-file ../../.env src/index.ts",
    "build": "tsc",
    "start": "node --env-file ../../.env dist/index.js",
    "typecheck": "tsc --noEmit",
    "deploy:commands": "tsx --env-file ../../.env src/scripts/deployCommands.ts",
    "lint": "eslint src --ext .ts"
  },
  "dependencies": {
    "@discordbot/shared": "workspace:*",
    "@discordbot/addon-sdk": "workspace:*",
    "@prisma/client": "^5.22.0",
    "discord.js": "^14.18.0",
    "ioredis": "^5.4.1",
    "bullmq": "^5.13.0",
    "axios": "^1.7.0",
    "pino": "^9.5.0",
    "pino-pretty": "^13.0.0",
    "dotenv": "^16.4.0",
    "ms": "^2.1.3",
    "@distube/ytdl-core": "^4.15.0",
    "play-dl": "^1.9.7",
    "@discordjs/voice": "^0.18.0",
    "libsodium-wrappers": "^0.7.15",
    "opusscript": "^0.1.1"
  },
  "devDependencies": {
    "@types/ms": "^0.7.34",
    "@types/node": "^22.0.0",
    "typescript": "^5.7.0",
    "tsx": "^4.19.0"
  }
}
